{
    "name": "rippleffect/admin-bundle",
    "type": "symfony-bundle",
    "description": "Rippleffect Administration Bundle",
    "homepage": "http://github.com/rippleffect/RippleAdminBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "James Halsall",
            "email": "jhalsall@rippleffect.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/Rippleffect/RippleUserBundle"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.10",
        "rippleffect/user-bundle": "1.0.*",
        "sonata-project/core-bundle": "2.2.*",
        "sonata-project/admin-bundle": "2.2.*",
        "sonata-project/doctrine-orm-admin-bundle": "2.2.*",
        "sonata-project/block-bundle": "2.2.*",
        "sonata-project/cache-bundle": "2.1.*",
        "sonata-project/intl-bundle": "2.2.*",
        "sonata-project/user-bundle": "2.2.*"
    },
    "autoload": {
        "psr-0": { "Ripple\\AdminBundle": "" }
    },
    "target-dir": "Ripple/AdminBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}

